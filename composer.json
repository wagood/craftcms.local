{
  "require": {
    "craftcms/cms": "^3.1.30",
    "craftcms/contact-form": "^2.2",
    "craftcms/contact-form-honeypot": "^1.0",
    "craftcms/redactor": "2.3.3.2",
    "ether/tags": "^1.0.4",
    "nystudio107/craft-seomatic": "^3.2",
    "pennebaker/craft-architect": "^2.3",
    "putyourlightson/craft-blitz": "^2.1",
    "selvinortiz/doxter": "^3.4",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability":"beta"
}
